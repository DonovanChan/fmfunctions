Let ( [
	$fn_list = listToDelimit ;
	quoted = ListCustom ( 1 ; ValueCount ( $fn_list ) ; "SQLString( GetValue( $fn_list ; [n] ) )" )
	] ;
	"( " & Substitute ( quoted ; ¶ ; "," ) & " )"
)

/* __________________________________________________

NAME:		SQLList ( listToDelimit )
PURPOSE:	Converts return-delimited list into comma-delimited list for use in SQL statements.
EXAMPLES:
	SQLList ( "dog¶cat¶mouse" ) = "(dog,cat,mouse)"
HISTORY:
	Created:	2011-10-26 16:23 PST - Donovan A. Chandler
*/