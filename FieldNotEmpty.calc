Let ( [	$fields = listOfFieldNames ;	ex =		"Let ( [ value = GetField ( GetValue ( $fields ; [n] ) )		] ; If ( value = \"?\" ; \"?\" ; not IsEmpty ( value ) )		)" ;	result = ListCustom ( 1 ; ValueCount ( $fields ) ; ex )	] ;	If (		Position ( result ; "?" ; 1 ; 1 ) ; "?" ;		Evaluate ( Substitute ( result ; ¶ ; "+" ) )	))/* —————————————————————————————— //NAME:	FieldNotEmpty ( listOfFieldNames )PURPOSE:	Given list of field names, returns count of fields that contain a value.EXAMPLES:	FieldNotEmpty ( List (		GetFieldName ( CONTACT::NAME_FIRST ) ;		GetFieldName ( CONTACT::NAME_LAST ) ;		GetFieldName ( CONTACT::TITLE )	) )	= 1 if NAME_FIRST is the only field with a value	= "?" if fields are not relatedHISTORY:	Created: 2011-Sep-30 13h55 PST — Donovan A. Chandler*/