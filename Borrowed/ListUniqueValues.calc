Let (
	[
		#_list = text ;
		#_value = GetValue ( #_list ; 1 ) ;
		#_next_values = RightValues ( #_list ; ValueCount ( #_list ) - 1 )      // all values except first to be passed in recursion
	] ;

	Case (
		#_list = ""
		; ""

		; 	If (
				ValueCount ( FilterValues ( #_list ; #_value ) ) > 1
				;  ""
				; #_value & "Â¶"
			) & uniqueValues ( #_next_values ) 

	)

)



